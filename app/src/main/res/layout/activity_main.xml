<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="fcmdemo.peacock.example.com.dummy.MainActivity">



    <TimePicker

        android:id="@+id/simpleTimePicker" android:scaleY="1.5" android:scaleX="1.5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:timePickerMode="spinner"/>
    <fcmdemo.peacock.example.com.dummy.NumberPicker
        android:id="@+id/test"
        style="@style/NumberPickerText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></fcmdemo.peacock.example.com.dummy.NumberPicker>

</LinearLayout>
